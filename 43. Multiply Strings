//https://leetcode.com/problems/multiply-strings/submissions/

class Solution {
public:
       string multiply(string num1, string num2) {
        if (num1=="0" or num2=="0")
            return "0";
        int l1=num1.size();
        int l2=num2.size();
        vector<int>result(l1+l2,0);
       // Умножаем в столбик
        for(int i=l1-1;i>-1;i--){
            for (int j=l2-1;j>-1;j--){
                int multy=(num1[i]-'0')*(num2[j]-'0');
                result[i+j+1]+=multy;
            }         
        }
         //сдвиг
        int k=0;  
        int p=0;  
        for(int i=l1+l2-1;i>-1;i--){
            k=(result[i]+p)%10;
            p=(result[i]+p)/10;
            result[i]=k;
        }
        string output;
        for (int i=0; i<l1+l2;i++)
            output+=to_string(result[i]);
        int i=0;
        while(output[i]=='0' && i<output.size()-1)
            i++;
        output= output.substr(i);
        return output;
                      
    }
};
